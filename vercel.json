{
    "version": 2,
    "builds": [
      {
        "src": "src/app.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/app.ts"
      }
    ],
    "functions": {
      "src/app.ts": {
        "maxDuration": 10
      }
    }
  }
  
